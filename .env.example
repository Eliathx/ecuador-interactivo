# ElevenLabs API Configuration
# Copy this file to .env and add your actual API key

# Get your API key from: https://elevenlabs.io/app/speech-synthesis/text-to-speech
VITE_ELEVENLABS_API_KEY=your_elevenlabs_api_key_here

# Voice ID - Use Rachel (English) by default, or find others at https://elevenlabs.io/voices
VITE_ELEVENLABS_VOICE_ID=21m00Tcm4TlvDq8ikWAM

# Model ID - eleven_multilingual_v2 is recommended for stability
VITE_ELEVENLABS_MODEL_ID=eleven_multilingual_v2

# Development settings
VITE_DEV_MODE=true
